    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "documentation": "^13.2.5",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "faker": "^5.5.3",
    "mocha": "^8.4.0",
    "mocha-logger": "^1.0.7",
