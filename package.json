{
  "name": "@rizefinance/rize-js",
  "version": "0.0.1",
  "description": "Rize API wrapper",
  "main": "index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix",
    "typings:build": "npx typescript",
    "docs:build": "documentation build lib/** -f md -o docs.md",
    "test": "yarn lint && yarn mocha $1",
    "local:publish": "yarn typings:build && yarn docs:build && npm unpublish --force --registry http://localhost @rizefinance/rize-js && npm publish --registry http://localhost"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jsrsasign": "^8.0.9",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "documentation": "^13.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "faker": "^5.4.0",
    "mocha": "^8.3.0",
    "mocha-logger": "^1.0.7",
    "ssn": "^1.0.3",
    "typescript": "^4.1.5",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jsrsasign": "^10.1.11",
    "query-string": "^6.14.0",
    "validator": "^13.5.2"
  },
  "repository": {
    "url": "git://github.com/RizeFinance/rize-sdks.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
